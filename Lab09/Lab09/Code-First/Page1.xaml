<Page x:Class="Lab09.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Lab09"
      mc:Ignorable="d" 
     d:DesignHeight="450" d:DesignWidth="800"
      Title="PageEx1">

    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="0.15*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Cursor="Hand" Content="Назад" HorizontalAlignment="Left" Width="80" Height="20" Background="Teal" Foreground="White"  x:Name="btnBack" Click="btnBack_Click"/>
        <Button Cursor="Hand" Content="Вперёд" x:Name="btnNext" HorizontalAlignment="Left" Background="Teal" Foreground="White"  Width="80" Height="20" Margin="90 0 0 0" Click="btnNext_Click"/>

        <Frame Grid.Row="1" Margin="10" x:Name="FrameForTable" NavigationUIVisibility ="Hidden"/>

    </Grid>
</Page>

